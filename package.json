{
  "name": "SimonGame",
  "version": "1.0.0",
  "description": "Simon Game",
  "repository": "https://github.com/minahalim/simongame.git",
  "main": "index.js",
  "scripts": {
    "lint": "npm run lint:js && npm run lint:css",
    "lint:css": "stylelint \"./src/**/*.scss\" \"./src/**/*.css\" --syntax scss",
    "lint:css:fix": "npm run lint:css -- --fix",
    "lint:js": "eslint .",
    "lint:js:fix": "npm run lint:js -- --fix",
    "clean": "rm -rf ./public/build",
    "start": "npm run build:prod && npm run server:prod",
    "build:watch": "npm run clean && NODE_ENV=development webpack -d --watch",
    "build:dev": "npm run clean && NODE_ENV=development webpack -d",
    "build:prod": "npm run clean && NODE_ENV=production webpack -p",
    "server:dev": "NODE_ENV=development node ./src/server.js",
    "server:prod": "NODE_ENV=production node ./src/server.js"
  },
  "author": "Mina Halim Hanna",
  "license": "ISC",
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "body-parser": "^1.18.2",
    "connect-history-api-fallback": "^1.5.0",
    "css-loader": "^0.28.8",
    "dotenv": "^4.0.0",
    "ejs": "^2.5.7",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-alias": "^1.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.9.1",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "getenv": "^0.7.0",
    "isdev": "^1.0.1",
    "node-sass": "^4.7.2",
    "prettier": "^1.13.6",
    "prop-types": "^15.6.0",
    "react": "^16.4.1",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^4.0.0-beta.15",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-sigma": "^1.2.30",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^2.0.4",
    "webpack-dev-server": "^2.11.1",
    "webpack-hot-middleware": "^2.21.0",
    "webpack-merge": "^4.1.1",
    "winston": "^2.4.0"
  }
}
